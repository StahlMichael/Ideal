{
  "name": "application-api",
  "version": "1.0.0",
  "description": "A basic Node.js + Express project",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node --use-system-ca index.js",
    "dev": "node --use-system-ca index.js",
    "watch": "nodemon --use-system-ca --watch src --ext ts --exec \"npm run build && node --use-system-ca dist/server.js\"",
    "legacy": "node --use-system-ca index.js"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "express": "^4.18.2",
    "mssql": "^12.2.0",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/mssql": "^9.1.5",
    "@types/node": "^20.10.6",
    "nodemon": "^3.1.11",
    "typescript": "^5.3.3"
  }
}
